-- mulitplies the numbers in the variables x and y, and saves the result in the variable xTimesY
x       := 76;
y       := 63786458;
xTimesY := 0;
xTemp   := x;
while !xTemp = 0 do
	if xTemp > 0 then
		xTimesY := xTimesY + y;
		xTemp   := xTemp - 1
	else
		xTimesY := xTimesY - y;
		xTemp   := xTemp + 1